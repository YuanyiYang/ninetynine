<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:VerticalPanel>
		<g:HorizontalPanel height="100px" ui:field="opponentArea"></g:HorizontalPanel>
		<g:HorizontalPanel height="100px" ui:field="usedArea"></g:HorizontalPanel>
		<g:HorizontalPanel height="100px" ui:field="unUsedArea"></g:HorizontalPanel>
		<g:HorizontalPanel height="100px" ui:field="playerArea"></g:HorizontalPanel>
		<g:HorizontalPanel height="100px">
			<g:HorizontalPanel width="120px" height="100px"
				ui:field="selectedArea"></g:HorizontalPanel>
			<g:Button ui:field="submitButton" text="Finish selecting card"></g:Button>
			<g:Button ui:field="subtractButton" text="Next Move Sub"></g:Button>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:HorizontalPanel height="100px" width="100px" ui:field="points"></g:HorizontalPanel>
			<g:HorizontalPanel height="100px" width="100px" ui:field="direction"></g:HorizontalPanel>
			<g:HorizontalPanel height="100px" width="100px" ui:field="mesg"></g:HorizontalPanel>
		</g:HorizontalPanel>
		<g:AbsolutePanel ui:field="dndArea">
		  
      <g:Label ui:field="dragMe">Drag Me</g:Label>
      <g:Label ui:field="dragOntoMe">Drag Onto Me</g:Label>
    </g:AbsolutePanel>
	</g:VerticalPanel>
</ui:UiBinder> 